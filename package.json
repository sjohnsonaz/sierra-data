  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/scripts/SierraData.js",
  "typings": "dist/scripts/SierraData.d.ts",
  "scripts": {
    "start": "node ./dist/scripts/SierraData.js",
    "build": "tsc",
