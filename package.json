  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "@types/mongodb": "3.1.9",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "mongodb": "3.1.6",
    "nodemon": "1.18.4",
    "rimraf": "2.6.2",
    "sierra": "^0.3.13",
    "typescript": "3.1.1"
  }
