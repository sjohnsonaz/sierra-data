    "@types/mongoose": "4.7.28",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "mongoose": "4.13.5",
    "nodemon": "1.12.1",
    "rimraf": "^2.6.2",
    "sierra": "0.3.1",
    "typescript": "2.6.2"
  }
}